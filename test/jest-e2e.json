{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/nest-grammy/(.*)": "<rootDir>/../libs/nest-grammy/src/$1",
    "@app/nest-grammy": "<rootDir>/../libs/nest-grammy/src",
    "@app/radarr-client/(.*)": "<rootDir>/../libs/radarr-client/src/$1",
    "@app/radarr-client": "<rootDir>/../libs/radarr-client/src"
  }
}